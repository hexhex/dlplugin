% -*- prolog -*-

#namespace(w, "http://wires#")

over(X) :- &dlC["wires.owl",a,b,c,d,"w:HighTrafficNode"](X).

known(X) :- &dlC["wires.owl",a,b,c,d,"w:Node"](X).

tr("w:wired",X,Y) :- &dlCQ["wires.owl",a,b,c,d, "w:wired(X,Z),w:wired(Z,Y)"](X,Y).
tr("w:wired",X,Y) :- &dlCQ["wires.owl",a,b,tr,d,"w:wired(X,Z),w:wired(Z,Y)"](X,Y), known(X), known(Y).
tran(X,Y) :- tr(_,X,Y).

foo(Y) :- &dlCQ["wires.owl",a,b,tr,d,"  w:wired(X,Z), w:wired(Z,Y), w:HighTrafficNode(Z)   "](_,Y).
foo2(Y) :- &dlCQ["wires.owl",a,b,tr,d,"w:wired(X,Z),w:wired(Z,Y)"](_,Y).
foo3(X,Y) :- &dlCQ["wires.owl",a,b,c,d,"w:wired(X,Z1),w:wired(Z1,Z2),w:wired(Z2,Z3),w:wired(Z3,Y)"](X,Y).

moo :- &dlCQ["wires.owl",a,b,tr,d,"w:wired(X,Y),w:HighTrafficNode(X),w:HighTrafficNode(Y)"].

